{
  "extends": [
    "expo/tsconfig.base",
    "@weebapp/typescript-config/base.json"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ]
    },
    "jsx": "react-native",
    "types": [
      "nativewind/types"
    ]
  },
  "include": [
    "app",
    "*.ts",
    "*.js",
    "components",
    "lib",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
