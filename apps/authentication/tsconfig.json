{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"lib": ["ESNext"],
		"types": ["@cloudflare/workers-types"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx"
	},
	"include": ["src/**/*.ts", "src/**/*.tsx", ".eslintrc.js"],
	"exclude": ["node_modules", "dist", "src/**/*.test.ts", "src/**/*.test.tsx"]
}
